<launch>    
    <arg name="map_size_x"/>
    <arg name="map_size_y"/>
    <arg name="map_size_z"/>
    <arg name="run_mode"/>
    <!-- 定义参数，从配置文件读取 -->
    <include file="$(find co_navi_pkg)/launch/high_speed/single_drone.launch">
        <arg name="drone_id"    value="0"/>
        <arg name="init_x"      value="-300.0"/>
        <arg name="init_y"      value="0.0"/>
        <arg name="init_z"      value="5.0"/>

        <arg name="x_target"    value="300.0"/>
        <arg name="y_target"    value="0.0"/>
        
        <arg name="a_max"       value="13.0"/>
        <arg name="v_final"     value="40.0"/>

        <arg name="map_size_x"  value="$(arg map_size_x)"/>
        <arg name="map_size_y"  value="$(arg map_size_y)"/>
        <arg name="map_size_z"  value="$(arg map_size_z)"/>
        <arg name="run_mode"    value="$(arg run_mode)"/>
    </include>
